        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Activator.Views.Ref_Person"
        mc:Ignorable="d"
        Title="ConfirmDeleteRef" Height="350" Width="800">
    <Grid>
        <Label Content="Click 'Confirm' to delete selected references." FontSize="38" Margin="10,38,9.6,206.8"></Label>
        <Button x:Name="ConfirmButton" Content="Confirm" Background="Red" Height="Auto" Margin="144,215,410.6,53.8" Click="ConfirmButton_Click" ></Button>
        <Button x:Name="CancelButton" Content="Cancel" Background="Green" Height="Auto" Margin="511,215,43.6,53.8" Click="CancelButton_Click" ></Button>

    </Grid>
</Window>
